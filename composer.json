{
    "name": "joedixon/nova-translation",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "joedixon/laravel-translation": "^2.0.0"
    },
    "require-dev": {
        "laravel/nova": "^4"
    },
    "autoload": {
        "psr-4": {
            "Joedixon\\NovaTranslation\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Joedixon\\NovaTranslation\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}